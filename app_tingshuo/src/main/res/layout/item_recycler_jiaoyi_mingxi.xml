<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:background="@color/white"
              android:layout_width="match_parent"
              android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_mingxi_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="YYQY"
        android:textColor="@color/black_222"
        android:textSize="30px"
        android:layout_marginTop="30px"
        android:layout_marginLeft="24px"
        />
    <TextView
        android:id="@+id/tv_mingxi_yue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="余额: 123.00"
        android:textColor="@color/black_222"
        android:textSize="24px"
        android:layout_marginTop="16px"
        android:layout_marginBottom="30px"
        android:layout_marginLeft="30px"
        android:layout_below="@id/tv_mingxi_type"/>
    <TextView
        android:id="@+id/tv_mingxi_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="余额: 123.00"
        android:textColor="@color/gray_93"
        android:textSize="24px"
        android:layout_marginTop="30px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24px"
        />
    <TextView
        android:id="@+id/tv_mingxi_money"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="-100.00"
        android:layout_below="@id/tv_mingxi_date"
        android:textColor="@color/black_0a0a0a"
        android:textSize="30px"
        android:layout_marginTop="16px"
        android:layout_alignParentRight="true"
        android:layout_marginRight="24px"
        />

</RelativeLayout>